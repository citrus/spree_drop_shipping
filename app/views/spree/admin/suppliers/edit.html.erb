<%= render "spree/admin/shared/supplier_tabs", :current => "Edit Supplier" %>

<%= render 'spree/shared/error_messages', :target => @supplier %>

<%= form_for([:admin, @supplier]) do |f| %>
  <%= render "form", :form => f %>
  <p class="form-buttons">
    <%= button t("update") %> <%= t('or') %> <%= link_to t('cancel'), collection_url %>
  </p>
<% end %>